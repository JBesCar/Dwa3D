<launch>
  <!-- Define the controller manager plugin to use for trajectory execution -->
  <param name="moveit_controller_manager" value="moveit_ros_control_interface::MoveItControllerManager" /> <!--moveit_simple_controller_manager/MoveItSimpleControllerManager-->

  <!-- loads controller list to the param server -->
  <rosparam file="$(find tyrion_moveit)/config/ros_controllers.yaml"/>
</launch>
